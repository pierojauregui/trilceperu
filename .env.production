# Configuración para Producción

# API URL para producción - trilceperu.com
VITE_API_URL=https://trilceperu.com/api

# Opción 2: Con IP del servidor (temporal, solo para pruebas iniciales)
# VITE_API_URL=http://45.130.104.37:8000/api

# Opción 3: Con subdominio API (si deseas usarlo en el futuro)
# VITE_API_URL=https://api.trilceperu.com

# Culqi - Llave Pública para PRODUCCIÓN
VITE_CULQI_PUBLIC_KEY=pk_live_CAMBIAR_POR_TU_LLAVE_PUBLICA_LIVE

# Configuración de Logging para Producción
# IMPORTANTE: En producción, los logs están deshabilitados por seguridad
VITE_LOG_LEVEL=ERROR
VITE_ENABLE_CONSOLE_LOGS=false
VITE_SHOW_SENSITIVE_DATA=false

# Configuración adicional para producción
VITE_NODE_ENV=production
VITE_DEBUG_MODE=false